<config
        xmlns='http://www.ehcache.org/v3'
>


    <cache alias="categories-cache" uses-template="heap-cache">
    </cache>


    <cache-template name="heap-cache">
        <resources>
            <heap unit="entries">100</heap>
            <offheap unit="MB">10</offheap>
        </resources>
    </cache-template>
</config>